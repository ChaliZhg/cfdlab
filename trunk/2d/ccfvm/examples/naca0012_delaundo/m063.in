grid
{
   type   delaundo
   file   naca.dpl
   cell   median
}

numeric
{
   time_mode    steady
   time_scheme  ssprk3
   time_step    0.0
   cfl          0.8
   max_iter     25000
   final_time   1.0e20
   min_residue  1.0e-6
   reconstruct  second
   bc_scheme    weak
   nitsche_pen  0
   smooth_res   no
   ducros       no
}

material
{
   gamma       1.4
   gas_const   1.0
   viscosity
   {
      model    constant
      mu_ref   0.0
   }
   prandtl     1.0
   model       euler
   flux        roe
}

constants
{
  rho  1.0
  p    1.79966166360724
  u    9.99390827019096e-01
  v    3.48994967025010e-02
}

initial_condition
{
   density      rho
   xvelocity    u
   yvelocity    v
   zvelocity    0.0
   pressure     p
}

boundary
{
   3 // airfoil surface
   {
      type         slip
   }

   5 // outer circle
   {
      type         farfield
      density      rho
      xvelocity    u
      yvelocity    v
      zvelocity    0.0
      pressure     p
   }

}

integrals
{
   force
   {
      airfoil
      3
   }
}

output
{
   format     vtk
   frequency  100
   variables
   {
      mach
   }
   surfaces
   {
      3
   }
   restart    true
   global_KE  false
}
