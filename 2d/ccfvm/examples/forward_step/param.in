grid
{
   type   gmsh 
   file   step.msh
}

numeric
{
   time_mode    unsteady
   time_scheme  ssprk3
   time_step    0.0
   cfl          0.9
   max_iter     50000
   final_time   4.0
   min_residue  1.0e-20
   reconstruct  bj
   smooth_res   no
   ducros       no
}

material
{
   gamma       1.4
   gas_const   7.14285714285714e-01 // R = 1/gamma
   viscosity
   {
      model   constant
      mu_ref  0.0
   }
   prandtl     1.0
   model       euler
   flux        kfvs
}

constants
{
   rho  1.4
   p    1.0
   u    3.0
}

initial_condition
{
   density      rho
   xvelocity    u
   yvelocity    0.0
   zvelocity    0.0
   pressure     p
}

boundary
{
   100001
   {
      type         farfield
      density      rho
      xvelocity    u
      yvelocity    0.0
      zvelocity    0.0
      pressure     p
   }

   100002
   {
      type         slip
   }

   100003
   {
      type         outlet
   }

}

output
{
   format     vtk
   frequency  500
   variables
   {
      mach
   }
   restart    false
   global_KE  false
}
